{
	"vueCompilerOptions": {
		"target": 2.7
	},
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": false,
		"noImplicitAny": false,
		"allowJs": true,
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"jsx": "preserve",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["ESNext", "DOM"],
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.js",
	],
	"exclude": [
		"node_modules",
		"vendor"
	]
}
